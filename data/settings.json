{
  "zones": {
    "autoReload": true,
    "validateBeforeReload": false,
    "backupEnabled": false,
    "autoGeneratePTR": true
  },
  "bind": {
    "version": "Bind 9.18.28",
    "configPath": "/etc/bind",
    "zonesPath": "/etc/bind/zones",
    "namedConfLocal": "/etc/bind/named.conf.local",
    "namedConfOptions": "/etc/bind/named.conf.options"
  },
  "resolver": {
    "enabled": true,
    "forwarders": [
      "8.8.8.8",
      "1.1.1.1",
      "94.140.14.14",
      "94.140.15.15"
    ],
    "queryLogging": false,
    "cacheSize": "256M",
    "dnssecValidation": true,
    "adblock": {
      "enabled": true,
      "blocklistUrls": [
        "https://raw.githubusercontent.com/StevenBlack/hosts/master/hosts",
        "http://pgl.yoyo.org/adservers/serverlist.php?hostformat=bindconfig&showintro=0&mimetype=plaintext",
        "https://raw.githubusercontent.com/oznu/dns-zone-blacklist/master/bind/bind-nxdomain.blacklist",
        "https://adguardteam.github.io/AdGuardSDNSFilter/Filters/filter.txt",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/pro.txt",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/tif.txt",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/fake.txt",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/popupads.txt",
        "https://raw.githubusercontent.com/hagezi/dns-blocklists/main/rpz/spam-tlds-rpz.txt"
      ],
      "redirectTo": "0.0.0.0",
      "customDomains": [
        "wzb.eu"
      ],
      "wildcardEnabled": true,
      "wildcardDomains": [
        "*.hereyourhotlady.com",
        "*.sweetkissesclub.com",
        "*.sweetxladies.com"
      ]
    },
    "doh": {
      "enabled": true,
      "port": 443,
      "certPath": "/etc/ssl/certs/ndash.crt",
      "keyPath": "/etc/ssl/private/ndash.key"
    },
    "dot": {
      "enabled": true,
      "port": 853,
      "certPath": "/etc/ssl/certs/ndash.crt",
      "keyPath": "/etc/ssl/private/ndash.key"
    }
  }
}